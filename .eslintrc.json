{
  "extends": "airbnb-base",

  "parser": "babel-eslint",  

  "plugins": ["import"],
   
  "rules": {
    "comma-dangle": 0,    
    "linebreak-style": 0,
    "no-console": 0,
    "func-names": 0    
  },

  "globals": {
    "$": true,
    "jQuery": true,
    "document": true,
    "window": true,
    "localStorage": true,
    "sessionStorage": true
  }
}